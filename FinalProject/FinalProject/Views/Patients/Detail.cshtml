@model Patients
@{
    ViewData["Title"] = "Detail";
}

<div class="row">

    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">

                <h4>Xəstə adı: @Model.Name</h4>
                <div>
                    Xəstənin həkimi:
                    @foreach (Doctors item in ViewBag.Doctors)
                    {
            <div class="d-inline">@item.FullName, </div>}
                </div>
                <div> Əlaqə nömrəsi: @Model.Contact</div>
                <div> Gəlis səbəbi : @Model.Reason</div>
                <div> Gəliş vaxtı : @Model.Date</div>
         


                <a class="btn btn-primary my-3" asp-action="Index">Geriyə</a>
            </div>
        </div>
    </div>
</div>