@model Managment
@{ ViewData["Title"] = "Detail"; }

<div class="row">

    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">

                <div class="d-flex justify-content-center"> <img style="object-fit : cover; width: 350px; height: 250px;" src="~/images/managment/@Model.Image" /> </div>
                <h4> Adı : @Model.FullName</h4>
                <div> Vəzifəsi : @Model.Positions.Name</div>
                <div> Maaşı : @Model.Salary</div>
                <div> Ünvanı : @Model.Address</div>
                <div> Əlaqə telefonu: @Model.Contact</div>





                <a class="btn btn-secondary  btn-sm my-3" asp-action="Index">Geriyə</a>
            </div>
        </div>
    </div>
</div>



