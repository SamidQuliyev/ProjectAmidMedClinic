@model Incomes
@{ ViewData["Title"] = "Detail"; }

<div class="row">

    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">


                <div>
                    Miqdarı:@Model.Quantity
                </div>
                <div>
                    Səbəbi:@Model.Reason
                </div>
                <div>
                    Qeyd olunan vaxt:@Model.Date
                </div>
                <div>
                    Qeyd edən şəxs:@Model.ByWho

                </div>

                <a class="btn btn-primary my-3" asp-action="Index">Geriyə</a>
            </div>
        </div>
    </div>
</div>

